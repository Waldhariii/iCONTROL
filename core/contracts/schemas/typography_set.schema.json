{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "typography_set.v1",
  "title": "typography_set",
  "type": "object",
  "required": ["typography_id", "name", "overrides"],
  "additionalProperties": false,
  "properties": {
    "typography_id": {"type": "string"},
    "name": {"type": "string"},
    "overrides": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["token_key", "value"],
        "additionalProperties": false,
        "properties": {
          "token_key": {"type": "string"},
          "value": {},
          "units": {"type": "string"}
        }
      }
    }
  }
}
