{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "compatibility_version.v1",
  "title": "compatibility_version",
  "type": "object",
  "required": ["from_version", "to_version", "allowed", "requires_migration", "migration_ids"],
  "additionalProperties": false,
  "properties": {
    "from_version": {"type": "string"},
    "to_version": {"type": "string"},
    "allowed": {"type": "boolean"},
    "requires_migration": {"type": "boolean"},
    "migration_ids": {"type": "array", "items": {"type": "string"}},
    "notes": {"type": "string"},
    "deprecation_flags": {"type": "array", "items": {"type": "string"}}
  }
}
