          "studio.*",
          "marketplace.*",
          "release.*",
          "runtime.read",
          "security.read",
          "tenancy.factory.*","ops.*","observability.read"
        ]
      },
      {
        "principal_id": "svc:ci",
        "scopes_allowlist": [
          "runtime.read",
          "marketplace.*",
          "billing.*",
          "security.read",
          "observability.read"
        ]
      },
      {
        "principal_id": "svc:ops",
        "scopes_allowlist": [
          "ops.*",
          "release.*",
          "runtime.read"
        ]
      },
      {
        "principal_id": "svc:integrations",
        "scopes_allowlist": [
          "integrations.*",
          "runtime.read",
          "security.read"
        ]
      },
      {
        "principal_id": "svc:rotation",
