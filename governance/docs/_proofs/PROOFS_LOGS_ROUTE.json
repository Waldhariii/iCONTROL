{
  "route": "#/logs",
  "page": {
    "path": "modules/core-system/ui/frontend-ts/pages/logs/index.ts",
    "export": "export function renderLogsPage(root: HTMLElement): void { void renderLogsPageAsync(root); }"
  },
  "dispatch": {
    "path": "app/src/moduleLoader.ts",
    "snippet": "if ((rid as any) === \"logs\") { ... import(\"../../modules/core-system/ui/frontend-ts/pages/logs\").then((m) => m.renderLogsPage(root))"
  },
  "nav": {
    "path": "platform-services/ui-shell/layout/shell.ts",
    "snippet": "{ id:\"logs\", label:\"Logs\", hash:\"#/logs\", show: ()=> isLoggedIn() && !isApp && canAccessPageRoute(\"logs\") }"
  },
  "dashboard_cta": {
    "path": "app/src/pages/cp/dashboard.ts",
    "snippet": "onClick: () => { window.location.hash = \"#/logs\"; }"
  },
  "build": {
    "command": "npm run build:cp",
    "status": "ok",
    "notes": "Warnings only: existing chunk/dynamic import warnings."
  },
  "checks": [
    {
      "id": "dispatch-moduleLoader",
      "path": "app/src/moduleLoader.ts",
      "mustContain": ["rid", "logs", "renderLogsPage(root)"]
    },
    {
      "id": "nav-shell",
      "path": "platform-services/ui-shell/layout/shell.ts",
      "mustContain": ["hash:\"#/logs\"", "id:\"logs\""]
    },
    {
      "id": "dashboard-cta",
      "path": "app/src/pages/cp/dashboard.ts",
      "mustContain": ["window.location.hash = \"#/logs\""]
    },
    {
      "id": "logs-export",
      "path": "modules/core-system/ui/frontend-ts/pages/logs/index.ts",
      "mustContain": ["export function renderLogsPage", "renderLogsPageAsync"]
    }
  ]
}
